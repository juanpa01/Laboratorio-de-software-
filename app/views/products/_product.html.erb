<h1>Productos</h1>
  <table class="responsive-table striped centered">
    <thead>
      <tr>
        <th>Nombre</th>
        <th>Descripcion</th>
        <th>Marca</th>
        <th>Categoria</th>
        <th>Cantidad</th>
        <th>Precio</th>
        <th colspan="2"></th>
      </tr>
    </thead>

    <tbody>
        <tr>
          <td><%= product.nombre %></td>
          <td><%= product.descripcion %></td>
          <td><%= product.marca %></td>
          <td><%= product.categoria %></td>
          <td><%= product.cantidad %></td>
          <td><%= product.precio %></td>
        </tr>
    </tbody>
  </table>
