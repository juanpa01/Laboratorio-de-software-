<div class="card-panel grey lighten-5">
   <div class="container">
     <div class="row">
        <%@tipo = "Editar producto" %>
       <h1>Editar Producto</h1>
       <%= render 'form', product: @product %>
       <div class="actions col s10">
         <%= link_to 'Mostrar', @product, class: "black-text text-darken-2" %> |
         <%= link_to 'Regresar', products_path, class: "black-text text-darken-2" %>
       </div>
     </div>
   </div>
</div>
